-- Popup Animations
local function playPopupAnimations()
    TweenService:Create(PopupGlow, TweenInfo.new(0.8, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1), {
        BackgroundTransparency = 0.95
    }):Play()
    TweenService:Create(PopupLabel, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1), {
        TextColor3 = Color3.fromRGB(255, 165, 0)
    }):Play()
end

local function showSwitchPopup()
    if SwitchPopup then
        SwitchPopup.Visible = true
        playPopupAnimations()
    end
end

local function hideSwitchPopup()
    if SwitchPopup then
        SwitchPopup.Visible = false
    end
end

-- ===================== UI Animation Effects =====================
local function addGlowEffect(frame, color)
    if not frame then return end
    local Glow = Instance.new("Frame")
    Glow.Name = "Glow"
    Glow.Parent = frame
    Glow.Size = UDim2.new(1.1, 0, 1.1, 0)
    Glow.Position = UDim2.new(-0.05, 0, -0.05, 0)
    Glow.BackgroundColor3 = color
    Glow.BackgroundTransparency = 0.8
    Glow.ZIndex = 98
    Glow.ClipsDescendants = true

    local GlowCorner = Instance.new("UICorner")
    GlowCorner.CornerRadius = UDim.new(0, 12)
    GlowCorner.Parent = Glow

    TweenService:Create(Glow, TweenInfo.new(1.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1), {
        BackgroundTransparency = 0.95
    }):Play()
end

addGlowEffect(TopTitleFrame, Color3.fromRGB(0, 255, 255))
addGlowEffect(WelcomeFrame, Color3.fromRGB(255, 100, 200))

-- Title Blink Animation
TweenService:Create(TitleLabel, TweenInfo.new(0.8, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1), {
    TextColor3 = Color3.fromRGB(0, 255, 255)
}):Play()

-- ===================== Core Functions =====================
-- Item Detection & Collection
local function findTargetItem()
    local GameFolder = Workspace:FindFirstChild("Game")
    if not GameFolder then return nil end

    local Entities = Game
