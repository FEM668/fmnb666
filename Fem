local LocalPlayer = Players.LocalPlayer

-- Character Refresh Listener (Fix: Character respawn null reference)
local Character, HumanoidRootPart, Humanoid
local function onCharacterAdded(newChar)
    Character = newChar
    HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
    Humanoid = Character:WaitForChild("Humanoid")
    -- Prevent script crash when character dies
    Humanoid.Died:Connect(function()
        Character = nil
        HumanoidRootPart = nil
        Humanoid = nil
    end)
end
-- Initialize character or listen for respawn
if LocalPlayer.Character then
    onCharacterAdded(LocalPlayer.Character)
end
LocalPlayer.CharacterAdded:Connect(onCharacterAdded)

-- Delta Support
local DeltaEnv = getgenv() or _G
DeltaEnv.FemHubEnabled = true

